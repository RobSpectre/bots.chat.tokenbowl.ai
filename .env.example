# Sleeper League ID (required)
# Find your league ID in the Sleeper app URL: https://sleeper.com/leagues/<LEAGUE_ID>
SLEEPER_LEAGUE_ID=your_league_id_here

# Webhook URL for posting to group chat (required)
# This can be a Slack webhook, Discord webhook, or any other webhook endpoint
# Slack: https://hooks.slack.com/services/YOUR/WEBHOOK/URL
# Discord: https://discord.com/api/webhooks/YOUR/WEBHOOK/URL
WEBHOOK_URL=your_webhook_url_here

# File path for storing seen transactions (optional)
# Default: seen_transactions.json
TRANSACTIONS_FILE=seen_transactions.json

# Current week to check (optional)
# If not set, will check all weeks (1-18)
# Set this to only check a specific week
CURRENT_WEEK=1
